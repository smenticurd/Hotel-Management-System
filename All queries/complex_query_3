--Find the total revenue generated by each room type, 
--including any additional charges and taxes:

SELECT rt.room_type_name, SUM(b.total_amount) AS total_revenue
FROM room_types rt
INNER JOIN rooms r ON rt.room_type_id = r.room_type_link_id
INNER JOIN hotels h ON h.hotel_id = r.hotel_link_id
INNER JOIN bookings b ON b.hotel_link_id = h.hotel_id
GROUP BY rt.room_type_name
